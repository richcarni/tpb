#: src/cfg.c:1570
#, c-format
msgid ""
"Usage: %s options\n"
"\n"
msgstr ""
"Verwendung: %s Optionen\n"
"\n"

#: src/cfg.c:1571
#, c-format
msgid "options:\n"
msgstr "Optionen:\n"

#: src/cfg.c:1572
#, c-format
msgid "   -h, --help           display this help\n"
msgstr "   -h, --help             Diese Hilfe anzeigen\n"

#: src/cfg.c:1573
#, c-format
msgid "   -d, --daemon         start in daemon mode\n"
msgstr "   -d, --daemon           Startet im Dämon Modus\n"

#: src/cfg.c:1574
#, c-format
msgid "   -c, --config=FILE    read FILE as additional configuration file\n"
msgstr "   -c, --config=DATEI     Liest DATEI als zusätzliche Konfigurationsdatei\n"

#: src/cfg.c:1575
#, c-format
msgid "   -m, --mixer=STATE    use OSS software mixer [%s]\n"
msgstr "   -m, --mixer=ZUSTAND    Benutzt den OSS Software Mixer [off]\n"

#: src/cfg.c:1577
#, c-format
msgid "   -o, --osd=STATE      show informations as on-screen display [%s]\n"
msgstr "   -o, --osd=ZUSTAND      Zeigt Informationen auf dem Bildschirm [on]\n"

#: src/cfg.c:1579
#, c-format
msgid "   -p, --poll=DELAY     set delay between polls in microseconds [%d]\n"
msgstr "   -p, --poll=ZYKLUS      Verzögerung zwischen Abfragen im Mikrosekunden [%d]\n"

#: src/cfg.c:1580
#, c-format
msgid "   -t, --thinkpad=CMD   string with command and options that should be executed\n"
msgstr "   -t, --thinkpad=KMD     Zeichenkette mit einem Kommando und Optionen, das ausgeführt werden soll,\n"

#: src/cfg.c:1581
#, c-format
msgid "                        when ThinkPad button is pressed [none]\n"
msgstr "                          wenn die ThinkPad Taste gedrückt wird [keines]\n"

#: src/cfg.c:1582
#, c-format
msgid "   -H, --home=CMD       command and options for Home button [none]\n"
msgstr "   -H, --home=KMD         Kommando und Optionen für die Home Taste [keines]\n"

#: src/cfg.c:1583
#, c-format
msgid "   -S, --search=CMD     command and options for Search button [none]\n"
msgstr "   -S, --search=KMD       Kommando und Optionen für die Suchen Taste [keines]\n"

#: src/cfg.c:1584
#, c-format
msgid "   -M, --mail=CMD       command and options for Mail button [none]\n"
msgstr "   -M, --mail=KMD         Kommando und Optionen für die Mail Taste [keines]\n"

#: src/cfg.c:1585
#, c-format
msgid "   -W, --wireless=CMD   command and options for Wireless button [none]\n"
msgstr "   -W, --wireless=KMD     Kommando und Optionen für die Wireless Taste [keines]\n"

#: src/cfg.c:1586
#, c-format
msgid "   -C, --callback=CMD   string with command that should be executed for each\n"
msgstr "   -C, --callback=KMD     Zeichenkette mit einem Kommando und Optionen, das für jede\n"

#: src/cfg.c:1587
#, c-format
msgid "                        pressed button. It is called with pressed button as\n"
msgstr "                          gedrückte Taste ausgeführt werden soll. Es wird mit der gedrückten Taste\n"

#: src/cfg.c:1588
#, c-format
msgid "                        first argument and new state as second [none]\n"
msgstr "                          als erstes Argument und dem neuen Zustand als zweites Argument aufgerufen [none]\n"

#: src/cfg.c:1589
#, c-format
msgid "   -A, --apm=STATE      poll /proc/apm for apm information [%s]\n"
msgstr "   -A, --apm=ZUSTAND      /proc/apm für apm Informationen abfragen [off]\n"

#: src/cfg.c:1590
#, c-format
msgid "   -P, --powermgt=STATE display of power management messages [%s]\n"
msgstr "   -P, --powermgt=ZUSTAND Zeigt Power Management Meldungen an [auto]\n"

#: src/cfg.c:1591
#, c-format
msgid "   -x, --xevents=STATE  grab X11 events of special keys [%s]\n"
msgstr "   -x, --xevents=STATE  Abfangen von X11 Ereignissen von Spezialtasten [on]\n"

#: src/cfg.c:1592
#, c-format
msgid "   -v, --verbose        print information about pressed keys\n"
msgstr "   -v, --verbose          Zeigt Informationen über gedrückte Tasten\n"
