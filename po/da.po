#: src/cfg.c:1353
#, c-format
msgid ""
"Usage: %s options\n"
"\n"
msgstr ""
"Brug: %s options\n"
"\n"

#: src/cfg.c:1354
#, c-format
msgid "options:\n"
msgstr "options:\n"

#: src/cfg.c:1355
#, c-format
msgid "   -h, --help           display this help\n"
msgstr "   -h, --help           vis denne hjælp\n"

#: src/cfg.c:1356
#, c-format
msgid "   -d, --daemon         start in daemon mode\n"
msgstr "   -d, --daemon         start i daemon mode\n"

#: src/cfg.c:1357
#, c-format
msgid "   -c, --config=FILE    read FILE as additional configuration file\n"
msgstr "   -c, --config=FILE    Læs FILE som yderligere konfigurations fil\n"

#: src/cfg.c:1358
#, c-format
msgid "   -m, --mixer=STATE    use OSS software mixer [off]\n"
msgstr "   -m, --mixer=STATE    Brug OSS software mixer [off]\n"

#: src/cfg.c:1360
#, c-format
msgid "   -o, --osd=STATE      show informations as on-screen display [on]\n"
msgstr "   -o, --osd=STATE      vis information som on-screen display [on]\n"

#: src/cfg.c:1362
#, c-format
msgid "   -p, --poll=DELAY     set delay between polls in microseconds [%d]\n"
msgstr "   -p, --poll=DELAY     sæt mellemrum mellem optællinger i mikrosekunder [%d]\n"

#: src/cfg.c:1363
#, c-format
msgid "   -t, --thinkpad=CMD   string with command and options that should be executed\n"
msgstr "   -t, --thinkpad=CMD   streng med kommando og options der skal køres\n"

#: src/cfg.c:1364
#, c-format
msgid "                        when ThinkPad button is pressed [none]\n"
msgstr "                        når ThinkPad button trykkes ned [ingen]\n"

#: src/cfg.c:1365
#, c-format
msgid "   -H, --home=CMD       command and options for Home button [none]\n"
msgstr "   -H, --home=CMD       kommando og options for Home knap [ingen]\n"

#: src/cfg.c:1366
#, c-format
msgid "   -S, --search=CMD     command and options for Search button [none]\n"
msgstr "   -S, --search=CMD     kommando og options for Search knap [ingen]\n"

#: src/cfg.c:1367
#, c-format
msgid "   -M, --mail=CMD       command and options for Mail button [none]\n"
msgstr "   -M, --mail=CMD       kommando og options for Mail knap [ingen]\n"

#: src/cfg.c:1368
#, fuzzy, c-format
msgid "   -W, --wireless=CMD   command and options for Wireless button [none]\n"
msgstr "   -M, --mail=CMD       kommando og options for Mail knap [ingen]\n"

#: src/cfg.c:1369
#, c-format
msgid "   -C, --callback=CMD   string with command that should be executed for each\n"
msgstr "   -C, --callback=CMD   streng med kommando der skal køres for hver\n"

#: src/cfg.c:1370
#, c-format
msgid "                        pressed button. It is called with pressed button as\n"
msgstr "                        trykket knap. Den kaldes med den trykkede knap som\n"

#: src/cfg.c:1371
#, c-format
msgid "                        first argument and new state as second [none]\n"
msgstr "                        første argument og ny tilstand som andet [ingen]\n"

#: src/cfg.c:1372
#, c-format
msgid "   -A, --apm=STATE      poll /proc/apm for apm information [off]\n"
msgstr "   -A, --apm=STATE      se i /proc/apm for apm information [fra]\n"

#: src/cfg.c:1373
#, c-format
msgid "   -P, --powermgt=STATE display of power management messages [auto]\n"
msgstr "   -P, --powermgt=STATE vis power management beskeder [auto]\n"

#: src/cfg.c:1374
#, c-format
msgid "   -x, --xevents=STATE  grab X11 events of special keys [on]\n"
msgstr " -x, --xevents=STATE   fang X11 hændelser af specielle taster [on]\n"

#: src/cfg.c:1375
#, c-format
msgid "   -v, --verbose        print information about pressed keys\n"
