#: src/cfg.c:1353
#, c-format
msgid ""
"Usage: %s options\n"
"\n"
msgstr ""
"Использование: %s опции\n"
"\n"

#: src/cfg.c:1354
#, c-format
msgid "options:\n"
msgstr "опции:\n"

#: src/cfg.c:1355
#, c-format
msgid "   -h, --help           display this help\n"
msgstr "   -h, --help           вызвать эту справку\n"

#: src/cfg.c:1356
#, c-format
msgid "   -d, --daemon         start in daemon mode\n"
msgstr "   -d, --daemon         запустить в виде демона\n"

#: src/cfg.c:1357
#, c-format
msgid "   -c, --config=FILE    read FILE as additional configuration file\n"
msgstr "   -c, --config=FILE    читать дополнительные настройки из файла FILE\n"

#: src/cfg.c:1358
#, c-format
msgid "   -m, --mixer=STATE    use OSS software mixer [off]\n"
msgstr "   -m, --mixer=STATE    использовать программный микшер OSS [off]\n"

#: src/cfg.c:1360
#, c-format
msgid "   -o, --osd=STATE      show informations as on-screen display [on]\n"
msgstr "   -o, --osd=STATE      показывать информацию на OSD [on]\n"

#: src/cfg.c:1362
#, c-format
msgid "   -p, --poll=DELAY     set delay between polls in microseconds [%d]\n"
msgstr "   -p, --poll=DELAY     установить интервал опроса в микросекундах [%d]\n"

#: src/cfg.c:1363
#, c-format
msgid "   -t, --thinkpad=CMD   string with command and options that should be executed\n"
msgstr "   -t, --thinkpad=CMD   строка с командой и её параметрами, которую выполнять\n"

#: src/cfg.c:1364
#, c-format
msgid "                        when ThinkPad button is pressed [none]\n"
msgstr "                        при нажатии клавиши ThinkPad [none]\n"

#: src/cfg.c:1365
#, c-format
msgid "   -H, --home=CMD       command and options for Home button [none]\n"
msgstr "   -H, --home=CMD       команда и параметры для клавиши Домой [none]\n"

#: src/cfg.c:1366
#, c-format
msgid "   -S, --search=CMD     command and options for Search button [none]\n"
msgstr "   -S, --search=CMD     команда и параметры для клавиши Поиск [none]\n"

#: src/cfg.c:1367
#, c-format
msgid "   -M, --mail=CMD       command and options for Mail button [none]\n"
msgstr "   -M, --mail=CMD       команда и параметры для клавиши Почта [none]\n"

#: src/cfg.c:1368
#, c-format
msgid "   -W, --wireless=CMD   command and options for Wireless button [none]\n"
msgstr "   -W, --wireless=CMD   команда и параметры для клавиши Антенна [none]\n"

#: src/cfg.c:1369
#, c-format
msgid "   -C, --callback=CMD   string with command that should be executed for each\n"
msgstr "   -C, --callback=CMD   строка с командой и параметрами, которую выполнять при\n"

#: src/cfg.c:1370
#, c-format
msgid "                        pressed button. It is called with pressed button as\n"
msgstr "                        нажатии любой клавиши. Вызывается с названием клавиши\n"

#: src/cfg.c:1371
#, c-format
msgid "                        first argument and new state as second [none]\n"
msgstr "                        и новым состоянием в качестве аргументов [none]\n"

#: src/cfg.c:1372
#, c-format
msgid "   -A, --apm=STATE      poll /proc/apm for apm information [off]\n"
msgstr "   -A, --apm=STATE      опрашивать /proc/apm для информации APM [off]\n"

#: src/cfg.c:1373
#, c-format
msgid "   -P, --powermgt=STATE display of power management messages [auto]\n"
msgstr "   -P, --powermgt=STATE показывать сообщения управления питанием [auto]\n"

#: src/cfg.c:1374
#, c-format
msgid "   -x, --xevents=STATE  grab X11 events of special keys [on]\n"
msgstr "   -x, --xevents=STATE  перехватывать события X11 от специальных клавиш [on]\n"

#: src/cfg.c:1375
#, c-format
msgid "   -v, --verbose        print information about pressed keys\n"
msgstr "   -v, --verbose        печатать информацию о нажатых клавишах\n"
